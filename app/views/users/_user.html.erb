<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Nickname:</strong>
    <%= user.nickname %>
  </p>

  <p>
    <strong>Password digest:</strong>
    <%= user.password_digest %>
  </p>

  <p>
    <strong>Admin rule:</strong>
    <%= user.admin_rule %>
  </p>

  <p>
    <strong>Employee:</strong>
    <%= user.employee_id %>
  </p>

</div>
